void exercise1()
{
	string text="5 He is good !!",word="\0",numb="5";
	int i;

	cout<<" "<<text.size();
	i=0;

		while(i<text.size())
	    {
			while(text[i]!= ' ')
		    {
			if(isdigit(text[i]))
			{
				cout<<text[i];
				i++;
				continue;
			}
			word=word+text[i];
			i++;
		    }
			i++;
			cout<<word;
	    }
}


