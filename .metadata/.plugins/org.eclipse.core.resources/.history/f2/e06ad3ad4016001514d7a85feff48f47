void exercise1()
{
	string text="5 He is good !!",word;
	int i;

	cout<<" "<<text.size();
	i=0;

		while(i<=text.size())
	    {
			word.clear();
			while(text[i]!= ' ')
		    {
				if(isdigit(text[i]))
				{
					cout<<text[i];
				}
				else
				{
					word=text[i];
				}
				i++;
		    }

			i++;
			cout<<word;
	    }
}


